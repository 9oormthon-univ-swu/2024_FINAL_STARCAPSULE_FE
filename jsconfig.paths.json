{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "@/components/*": [
                "src/components/*"
            ],
            "@/storybook/*": [
                "src/storybook/*"
            ],
            "@/utils/*": [
                "src/utils/*"
            ],
            "@/hooks/*": [
                "src/hooks/*"
            ],
            "@/page/*": [
                "src/page/*"
            ],
            "@/layout/*": [
                "src/layout/*"
            ],
            "@/constants/*": [
                "src/constants/*"
            ],
            "@/assets/*": [
                "src/assets/*"
            ],
            "@/stores/*": [
                "src/stores/*"
            ],
        }
    }
}